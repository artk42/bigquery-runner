  "scripts": {
    "dev": "run-p generate:watch",
    "generate": "run-p generate:readme generate:dts",
    "generate:watch": "nodemon --exec 'npm run generate'",
    "generate:readme": "ejs ./src/README.md.ejs -f ../../package.json -o ../../README.md",
    "generate:dts": "ejs ./src/config.d.ts.ejs -f ../../package.json -o ../core/src/config.d.ts"
  },
  "nodemonConfig": {
    "watch": [
      "./src/",
      "../../package.json"
    ],
    "ext": "ejs,json"
  }
}
