{
  "private": true,
  "name": "core",
  "version": "0.0.0",
  "description": "",
  "main": "src/extension.ts",
  "scripts": {
    "dev": "run-p compile:watch",
    "compile": "rm -rf ../../out && esbuild ./src/extension.ts --bundle --outfile=../../out/extension.js --external:vscode --format=cjs --platform=node",
    "compile:test": "tsc -p ./",
    "compile:watch": "npm run compile -- --sourcemap --watch",
    "compile:debug": "npm run compile -- --sourcemap",
    "compile:publish": "npm run compile -- --minify",
    "pretest": "npm run compile:test && npm run lint",
    "lint": "eslint 'src/**/*.ts'",
    "test": "node ../../out/test/runTest.js"
  },
  "devDependencies": {
    "@types/csv-stringify": "^3.1.0",
    "@types/easy-table": "^1.2.0",
    "@types/flat": "^5.0.2",
    "@types/mkdirp": "^1.0.2",
    "@types/vscode": "^1.63.1",
    "@vscode/test-electron": "^1.6.2"
  }
}
