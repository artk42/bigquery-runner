{
  "private": true,
  "name": "codegen",
  "version": "0.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon --exec 'npm run build'",
    "build": "run-p build:*",
    "build:readme": "ejs ./src/README.md.ejs -f ../../package.json -o ../../README.md",
    "build:dts": "ejs ./src/config.d.ts.ejs -f ../../package.json -o ../extension/src/config.d.ts"
  },
  "nodemonConfig": {
    "watch": [
      "./src/",
      "../../package.json"
    ],
    "ext": "ejs,json"
  }
}
